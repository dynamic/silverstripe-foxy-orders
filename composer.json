{
    "name": "dynamic/silverstripe-foxy-orders",
    "description": "Add order history to your SilverStripe Foxy store.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "Foxy.io",
        "ecommerce",
        "orders"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "http://www.dynamicagency.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "dynamic/silverstripe-foxy-feed-parser": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0",
        "silverstripe/recipe-cms": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Foxy\\Orders\\": "src/",
            "Dynamic\\Foxy\\Orders\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}